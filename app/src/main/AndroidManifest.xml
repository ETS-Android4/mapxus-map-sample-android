            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mapxus.mapxusmapandroiddemo.examples.basics.SimpleMapViewActivity"
            android:label="@string/activity_basic_simple_mapview_title">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.mapxus.mapxusmapandroiddemo.MainActivity" />
        </activity>
        <activity
            android:name="com.mapxus.mapxusmapandroiddemo.examples.basics.SupportMapFragmentActivity"
            android:label="@string/activity_basic_support_map_frag_title">
